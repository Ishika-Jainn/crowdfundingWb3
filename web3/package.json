{
  "name": "hardhat-javascript-starter",
  "scripts": {
    "build": "npx thirdweb@latest detect",
    "deploy": "dotenv npx thirdweb deploy -k %THIRDWEB_SECRET_KEY%",
    "check-key": "dotenv node -e \"console.log('My Secret Key is:', process.env.THIRDWEB_SECRET_KEY)\"",
    "start": "react-scripts start",
    "publish": "npx thirdweb@latest publish"
  },
  "devDependencies": {
    "@matterlabs/hardhat-zksync-solc": "^1.1.14",
    "@matterlabs/hardhat-zksync-verify": "^1.7.1",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "hardhat": "^2.19.1"
  },
  "dependencies": {
    "@thirdweb-dev/contracts": "^3.8.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "zksync-ethers": "^5.7.0"
  }
}
